<?xml version="1.0" ?>
<robot name="ackerman">

  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="1.0 0.4 0.1"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
    <inertial>
      <mass value="1" />
      <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <box size="1.0 0.4 0.1"/>
      </geometry>
    </collision>
  </link>

  <!-- Front Axle Link (Parallel to base) -->
  <link name="front_axle_link">
    <visual>
      <geometry>
        <cylinder radius="0.01" length="0.5"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
    <inertial>
      <mass value="0.5" />
      <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.01" length="0.5"/>
      </geometry>
    </collision>
  </link>

  <!-- Back Axle Link (Parallel to base) -->
  <link name="back_axle_link">
    <visual>
      <geometry>
        <cylinder radius="0.01" length="0.5"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
    <inertial>
      <mass value="0.5" />
      <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.01" length="0.5"/>
      </geometry>
    </collision>
  </link>

  <!-- Front Wheel Link -->
  <link name="front_left_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1" />
      </material>
    </visual>
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
  </link>

  <link name="front_right_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1" />
      </material>
    </visual>
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
  </link>

  <!-- Back Wheel Links -->
  <link name="back_left_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1" />
      </material>
    </visual>
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
  </link>

  <link name="back_right_wheel_link">
    <visual>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1" />
      </material>
    </visual>
    <inertial>
      <mass value="0.2" />
      <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
  </link>

  <!-- Front Axle Steering Joint (Rotation around Y-axis for steering) -->
  <joint name="front_axle_steering_joint" type="revolute">
    <parent link="base_link"/>
    <child link="front_axle_link"/>
    <origin xyz="0.4 0 0" rpy="1.57 0 0"/>
    <axis xyz="0 1 0"/> 
    <limit effort="100.0" lower="-0.4" upper="0.4" velocity="1.0"/>
  </joint>

  <!-- Back Axle Fixed Joint -->
  <joint name="back_axle_fixed_joint" type="fixed">
    <parent link="base_link"/>
    <child link="back_axle_link"/>
    <origin xyz="-0.4 0 0" rpy="1.57 0 0"/>
  </joint>

  <!-- Joints for Front Wheels  -->
  <joint name="front_wheel_joint" type="continuous">
    <parent link="front_axle_link"/>
    <child link="front_left_wheel_link"/>
    <origin xyz="0 0 -0.25" rpy="0 0 0"/>
    <axis xyz="0 0 1"/> 
  </joint>

  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="front_axle_link"/>
    <child link="front_right_wheel_link"/>
    <origin xyz="0.0 0.0 0.25" rpy="0 0 0"/>
    <mimic joint="front_wheel_joint" multiplier="1.0"/> 
    <axis xyz="0 0 1"/> 
  </joint>

  <!-- Joints for Back Wheels -->
  <joint name="back_left_wheel_joint" type="continuous">
    <parent link="back_axle_link"/>
    <child link="back_left_wheel_link"/>
    <origin xyz="0.0 0.0 -0.25" rpy="0 0 0"/>
    <mimic joint="front_wheel_joint" multiplier="1.0"/>
    <axis xyz="0 0 1"/> 
  </joint>

  <joint name="back_right_wheel_joint" type="continuous">
    <parent link="back_axle_link"/>
    <child link="back_right_wheel_link"/>
    <origin xyz="0.0 0.0 0.25" rpy="0 0 0"/>
    <mimic joint="front_wheel_joint" multiplier="1.0"/> 
    <axis xyz="0 0 1"/> 
  </joint>

</robot>

<?xml version="1.0" ?>
  <robot name="ackerman">



  <link name="base">
    <visual>
      <geometry>
          <box size="1.0 0.4 0.1"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <inertial>
        <mass value="1" />
        <inertia ixx="0.01" ixy="0.0" ixz="0" iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <collision>
      <geometry>
          <box size="0.75 0.4 0.1"/>
      </geometry>
    </collision>

  </link>






  <link name="wheel_right_link">
    <inertial>
            <mass value="2" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>

      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_right_joint" type="continuous">
    <origin xyz="0.4 0.25 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="base"/>
    <child link="wheel_right_link"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>



  <link name="wheel_left_link">
    <inertial>
            <mass value="2" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>

      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <origin xyz="0.4 -0.25 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="base"/>
    <child link="wheel_left_link"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>

<!---->

<link name="wheel_front_right_link">
    <inertial>
            <mass value="2" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>

      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_front_right_joint" type="continuous">
    <origin xyz="-0.4 0.25 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="base"/>
    <child link="wheel_front_right_link"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>



  <link name="wheel_front_left_link">
    <inertial>
            <mass value="2" />
            <inertia ixx="0.01" ixy="0.0" ixz="0"
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>

      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_front_left_joint" type="continuous">
    <origin xyz="-0.4 -0.25 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="base"/>
    <child link="wheel_front_left_link"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>
  

 
<!--arm-->


<!-- Arm Base Joint -->
<joint name="base_arm_joint" type="continuous">
    <origin xyz="0.0 0.0 0.05" rpy="0.0 0.0 0.0"/>
    <parent link="base"/>
    <child link="base_link"/>
    <axis xyz="0.0 0.0 1.0"/>
</joint>

<!-- Base link -->
<link name="base_link">
    <visual>
        <geometry>
            <cylinder radius="0.15" length="0.1"/>
        </geometry>
        <material name="blue">
            <color rgba="0 0 1 1"/>
        </material>
    </visual>
    <!-- Collision for base_link -->
    <collision>
        <geometry>
            <cylinder radius="0.15" length="0.1"/>
        </geometry>
    </collision>
    <!-- Inertia for base_link -->
    <inertial>
        <mass value="5.0"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
    </inertial>
</link>

<!-- Base joint (continuous joint) -->
<joint name="base_joint" type="continuous">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0.0 0.0 0.25" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
</joint>

<!-- Link 1 -->
<link name="link_1">
    <visual>
        <geometry>
            <cylinder radius="0.05" length="0.5"/>
        </geometry>
        <material name="green">
            <color rgba="0 1 0 1"/>
        </material>
    </visual>
    <!-- Collision for link_1 -->
    <collision>
        <geometry>
            <cylinder radius="0.05" length="0.5"/>
        </geometry>
    </collision>
    <!-- Inertia for link_1 -->
    <inertial>
        <mass value="1.0"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
</link>

<!-- Joint 2 (revolute) -->
<joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0 0.5" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
</joint>

<!-- Link 2 -->
<link name="link_2">
    <visual>
        <geometry>
            <cylinder radius="0.05" length="0.5"/>
        </geometry>
        <material name="red">
            <color rgba="1 0 0 1"/>
        </material>
    </visual>
    <!-- Collision for link_2 -->
    <collision>
        <geometry>
            <cylinder radius="0.05" length="0.5"/>
        </geometry>
    </collision>
    <!-- Inertia for link_2 -->
    <inertial>
        <mass value="0.8"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.008" ixy="0" ixz="0" iyy="0.008" iyz="0" izz="0.008"/>
    </inertial>
</link>

<!-- Joint 3 (revolute) -->
<joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0 0.35" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10" velocity="1.0"/>
</joint>

<!-- Link 3 -->
<link name="link_3">
    <visual>
        <geometry>
            <cylinder radius="0.05" length="0.2"/>
        </geometry>
        <material name="yellow">
            <color rgba="1 1 0 1"/>
        </material>
    </visual>
    <!-- Collision for link_3 -->
    <collision>
        <geometry>
            <cylinder radius="0.05" length="0.2"/>
        </geometry>
    </collision>
    <!-- Inertia for link_3 -->
    <inertial>
        <mass value="0.5"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.005"/>
    </inertial>
</link>

<!-- Finger 1 -->
<link name="finger_1">
    <visual>
        <geometry>
            <cylinder radius="0.01" length="0.1"/>
        </geometry>
        <material name="orange">
            <color rgba="1 0.5 0 1"/>
        </material>
    </visual>
    <!-- Collision for finger_1 -->
    <collision>
        <geometry>
            <cylinder radius="0.01" length="0.1"/>
        </geometry>
    </collision>
    <!-- Inertia for finger_1 -->
    <inertial>
        <mass value="0.05"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
</link>

<!-- Finger 2 -->
<link name="finger_2">
    <visual>
        <geometry>
            <cylinder radius="0.01" length="0.1"/>
        </geometry>
        <material name="purple">
            <color rgba="0.5 0 1 1"/>
        </material>
    </visual>
    <!-- Collision for finger_2 -->
    <collision>
        <geometry>
            <cylinder radius="0.01" length="0.1"/>
        </geometry>
    </collision>
    <!-- Inertia for finger_2 -->
    <inertial>
        <mass value="0.05"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
</link>

<!-- Prismatic joint for Finger 1 -->
<joint name="finger_joint_1" type="prismatic">
    <parent link="link_3"/>
    <child link="finger_1"/>
    <origin xyz="-0.03 0.0 0.15" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0" upper="0.02" effort="5" velocity="0.1"/>
</joint>

<!-- Prismatic joint for Finger 2 -->
<joint name="finger_joint_2" type="prismatic">
    <parent link="link_3"/>
    <child link="finger_2"/>
    <origin xyz="0.03 0.0 0.15" rpy="0 0 0"/>
    <axis xyz="-1 0 0"/>
    <limit lower="0" upper="0.02" effort="5" velocity="0.1"/>
</joint>


<gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="gazebo_base_controller">
      <odometry_frame>odom</odometry_frame>
      <commandTopic>cmd_vel</commandTopic>
      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
      <update_rate>15.0</update_rate>

      <left_joint>wheel_front_left_joint</left_joint>
      <right_joint>wheel_front_right_joint</right_joint>

      <wheel_separation>0.5</wheel_separation>
      <wheel_diameter>0.3</wheel_diameter>
      <max_wheel_acceleration>0.7</max_wheel_acceleration>
      <max_wheel_torque>8</max_wheel_torque>
      <robotBaseFrame>base</robotBaseFrame>
    </plugin>
  </gazebo>


</robot>